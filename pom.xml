    <name>unique Maven Webapp</name>


    <!-- 参数设置 -->
    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <!-- 设置 Spring 的版本 -->
        <spring.version>4.1.1.RELEASE</spring.version>
        <mybatis.version>3.2.7</mybatis.version>
